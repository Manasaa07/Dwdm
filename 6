#(iii) Normalization by decimal scaling

decimal_scaling_normalize <- function(x) {

j-max (abs(x))

k<- nchar(as.character(j))

return(x/ (10^k))

}

decimal_scaled_value <- decimal_scaling_normalize (age_value)

cat("Decimal scaling normalized value for age 35:\n")

ecimal scaling normalized value for age 35:

print (decimal_scaled_value)

1] 0.35
